.cpu mm-3

; данная программа считает дискриминант
; b*2 - 4a*c

.code
; тут пишем код
03 0101 0101 0103; b^2: 03 - умножение(без знака), 0101 - ячейка, в которой лежит b, а в 0103 мы пишем результат
03 0100 0102 0104; ac
03 0005 0104 0104; 4ac, 0005 - это адоес константы 4;
02 0103 0104 0105; b^2-4a*c: 02 - вычитание
99 0000 0000 0000; 99 - код остановки программы
00000000000004 ; а здесь описана константа 4

.input 0x100 a
.input 0x101 b
.input 0x102 c
.output 0x105 d
.enter 1 4 1
